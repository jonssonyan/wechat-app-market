<view>
    <block wx:if="{{orders.length!==0}}">
        <i-card wx:for="{{orders}}" wx:key="*this" data-order="{{item}}" title="{{item.productName}}"
                bindtap="cardClick"
                full="true">
            <view slot="content">创建时间：{{item.create_time}}</view>
        </i-card>
    </block>
    <block wx:else>
        <i-alert>
            暂无购买，快去买一件吧
        </i-alert>
    </block>
</view>