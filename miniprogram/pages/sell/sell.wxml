<!--我的卖出-->
<view>
    <block wx:if="{{orders.length!==0}}">
        <i-card wx:for="{{orders}}" wx:key="*this" data-order="{{item}}" title="{{item.products[0].name}}"
                bindtap="cardClick"
                full="true">
            <view slot="content">创建时间：{{item.createTime}}</view>
        </i-card>
    </block>
    <block wx:else>
        <i-alert>加载中...</i-alert>
    </block>
</view>