<view>
    <block wx:if="{{orders.length!==0}}">
        <i-card wx:for="{{products}}" wx:key="*this" data-product="{{item}}" title="{{item.name}}" bindtap="cardClick"
                full="true">
            <view slot="content">创建时间：{{item.create_time}}</view>
        </i-card>
    </block>
    <block wx:else>
        <i-alert>暂无商品，快去添加一件吧</i-alert>
    </block>
</view>