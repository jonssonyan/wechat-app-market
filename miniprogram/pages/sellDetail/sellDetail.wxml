<i-cell-group>
    <i-input value="{{ order.product[0].name }}" title="商品名称" disabled/>
    <i-input value="{{ order.address }}" title="收货地址" disabled/>
    <i-input value="{{ order.num }}" title="数量" disabled/>
    <i-input value="{{ order.total_price +'元'}}" title="总价" disabled/>
    <i-input value="{{ order.createTime }}" title="卖出时间" disabled/>
    <i-input value="{{ order.user[0].nickName }}" title="购买人" disabled/>
</i-cell-group>